{
  "name": "mmm-checklist",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/x-date-pickers": "^6.19.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.9.0",
    "cacache": "^19.0.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.16.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-router": "^7.6.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^3.0.1",
    "recharts": "^2.15.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:production": "set NODE_OPTIONS=--openssl-legacy-provider && set GENERATE_SOURCEMAP=false && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:check": "eslint src/ --max-warnings 1000",
    "lint:cache-clear": "rm -rf .eslintcache && eslint --cache src/",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "format:fix": "npm run format",
    "analyze:size": "npx source-map-explorer 'build/static/js/*.js'",
    "performance:test": "npm run build:production && npm run analyze:size",
    "quality:check": "npm run lint:check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format:fix",
    "health:check": "npm run quality:check && npm run test -- --watchAll=false --passWithNoTests",
    "precommit": "npm run quality:fix",
    "analyze:deps": "npm ls --depth=0",
    "analyze:security": "npm audit",
    "analyze:bundle": "npm run build && npm run analyze:size",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "neo-async": "^2.6.2",
    "postcss-double-position-gradients": "^6.0.2",
    "prettier": "^3.5.3",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "proxy": "http://localhost:5000"
}
