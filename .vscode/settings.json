{
  "workbench.startupEditor": "none",
  "workbench.enableExperiments": false,
  "telemetry.enableTelemetry": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.hover.enabled": false,
  "editor.lightbulb.enabled": "off",
  "editor.quickSuggestions": false,
  "editor.parameterHints.enabled": false,
  "editor.minimap.enabled": false,
  "editor.glyphMargin": false,
  "editor.folding": false,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "editor.renderIndentGuides": false,
  "editor.occurrencesHighlight": false,
  "editor.selectionHighlight": false,
  "editor.wordHighlight": false,
  "editor.bracketPairColorization.enabled": false,
  "cursor.chat.maxModelContextLength": 200000,
  "cursor.chat.enableFileTree": false,
  "cursor.chat.enableWorkspaceSearch": true,
  "cursor.autocomplete.enable": true,
  "cursor.autocomplete.maxSuggestions": 3,
  "cursor.autocomplete.multilineCompletions": "enabled",
  "search.maxResults": 1000,
  "search.smartCase": true,
  "search.followSymlinks": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": false,
  "git.decorations.enabled": false,
  "problems.decorations.enabled": false,
  "eslint.run": "onSave",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  }
}
